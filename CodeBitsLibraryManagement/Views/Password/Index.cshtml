
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model Password

<h1>Login</h1>
<form>
    <div class="form-group">
        <label asp-for="UserName" class="col-sm-2 col-form-label">UserName</label>
        <div class="col-sm-10">
            <input asp-for="UserName" type="text" class="form-control" placeholder="user name" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Pasword" class="col-sm-2 col-form-label">Password</label>
        <div class="col-sm-10">
            <input asp-for="Pasword" type="password" class="form-control" placeholder="password" />
        </div>
    </div>

    <a href="" class="btn btn-secondary">Login</a>
</form>

